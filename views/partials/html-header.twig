<!doctype html>
<!--[if lt IE 7]><html class="no-js ie ie6 lt-ie9 lt-ie8 lt-ie7" {{site.language_attributes}}> <![endif]-->
<!--[if IE 7]><html class="no-js ie ie7 lt-ie9 lt-ie8" {{site.language_attributes}}> <![endif]-->
<!--[if IE 8]><html class="no-js ie ie8 lt-ie9" {{site.language_attributes}}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" {{site.language_attributes}}> <!--<![endif]-->
<head>
  <meta charset="{{site.charset}}" />
    <title>   
      {% if wp_title %}
        {{ wp_title }} - {{ site.name }}
      {% else %}
        {{ site.name }}
      {% endif %}
  </title>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <link rel="apple-touch-icon" sizes="180x180" href="{{ theme.path }}/assets/public/favicons/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ theme.path }}/assets/public/favicons/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ theme.path }}/assets/public/favicons/favicon-16x16.png">
  <link rel="manifest" href="{{ theme.path }}/assets/public/favicons/site.webmanifest">
  <link rel="mask-icon" href="{{ theme.path }}/assets/public/favicons/safari-pinned-tab.svg" color="#27405c">
  <meta name="msapplication-TileColor" content="#27405c">
  <meta name="theme-color" content="#f1f3f6">

  <link rel="pingback" href="{{site.pingback_url}}" />
  <link href="https://fonts.googleapis.com/css?family=Barlow:300,400,500" rel="stylesheet">
  {{function('wp_head')}}

<!-- Values section -->
<section class="values">
  <div class="values__container container">
    <div class="values__content container-xs">
      <div class="values__content__label label label-blue">
        <h4>{{page.about_sec3_surtitle}}</h4>
      </div>
      <h3 class="values__content__title">{{page.about_sec3_title}}</h3>
    </div>
    <div class="values__text-slider">
      {% include 'partials/textSlider.twig' %}
    </div>
  </div>
</section>
<!-- End values section -->

<!-- Differences section -->
<section class="about-differences">
  <div class="about-differences__container">
    {% if page.about_sec4_video %}
    <div class="about-video">
      {{page.get_field('about_sec4_video')}}
    </div>
    <div class="about-dual-cards dual-cards video--on container">
    {% else %}
    <div class="about-dual-cards dual-cards container">
    {% endif %}
      <div class="about-dual-cards__content dual-cards__content container-xs">
        <div class="about-dual-cards__content__label dual-cards__content__label label label-blue">
          <h4>{{page.about_sec5_surtitle}}</h4>
        </div>
        <h3 class="about-dual-cards__content__title dual-cards__content__title">{{page.about_sec5_title}}</h3>
      </div>
      <div class="about-dual-cards__wrapper dual-cards__wrapper container-xs">
        <div class="about-dual-cards__item dual-cards__item">
          <div class="about-dual-cards__item__img dual-cards__item__img">
            <img src="{{Image(page.about_sec5_business_img).src}}"  
          srcset="
          {{Image(page.about_sec5_business_img)|fit(image_size.retina.very_small.width, image_size.retina.very_small.height)}} {{image_size.retina.width}},
          {{Image(page.about_sec5_business_img)|fit(image_size.large.very_small.width, image_size.large.very_small.height)}} {{image_size.large.width}},
          {{Image(page.about_sec5_business_img)|fit(image_size.medium.very_small.width, image_size.medium.very_small.height)}} {{image_size.medium.width}},
          {{Image(page.about_sec5_business_img)|fit(image_size.mobile.very_small.width, image_size.mobile.very_small.height)}} {{image_size.mobile.width}}"
          alt="{{Image(page.about_sec5_business_img).alt}}">
          </div>
          <span class="about-dual-cards__item__title dual-cards__item__title">{{page.about_sec5_business_title}}</span>
          <ul class="about-dual-cards__item__description dual-cards__item__description">
            {% for item in page.get_field('about_sec5_business_list') %}
              <li>{{item.difference}}</li>
            {% endfor %}
          </ul>
        </div>
        <div class="about-dual-cards__item dual-cards__item">
          <div class="about-dual-cards__item__img dual-cards__item__img">
            <img src="{{Image(page.about_sec5_user_img).src}}"  
          srcset="
          {{Image(page.about_sec5_user_img)|fit(image_size.retina.very_small.width, image_size.retina.very_small.height)}} {{image_size.retina.width}},
          {{Image(page.about_sec5_user_img)|fit(image_size.large.very_small.width, image_size.large.very_small.height)}} {{image_size.large.width}},
          {{Image(page.about_sec5_user_img)|fit(image_size.medium.very_small.width, image_size.medium.very_small.height)}} {{image_size.medium.width}},
          {{Image(page.about_sec5_user_img)|fit(image_size.mobile.very_small.width, image_size.mobile.very_small.height)}} {{image_size.mobile.width}}"
          alt="{{Image(page.about_sec5_user_img).alt}}">
          </div>
          <span class="about-dual-cards__item__title dual-cards__item__title">{{page.about_sec5_user_title}}</span>
          <ul class="about-dual-cards__item__description dual-cards__item__description">
              {% for item in page.get_field('about_sec5_user_list') %}
                <li>{{item.difference}}</li>
              {% endfor %}
          </ul>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- End Video section -->

  
{% endblock %}
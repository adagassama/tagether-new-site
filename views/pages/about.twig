          {{Image(page.about_sec2_testimony_img)|fit(image_size.retina.very_small.width, image_size.retina.very_small.height)}} {{image_size.retina.width}},
          {{Image(page.about_sec2_testimony_img)|fit(image_size.large.very_small.width, image_size.large.very_small.height)}} {{image_size.large.width}},
          {{Image(page.about_sec2_testimony_img)|fit(image_size.medium.very_small.width, image_size.medium.very_small.height)}} {{image_size.medium.width}},
